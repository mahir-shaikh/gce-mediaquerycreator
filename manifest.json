{
    "manifest_version": 3,
    "name": "Media Query Creator",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle",
            "all_frames": false
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}